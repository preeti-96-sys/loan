<p id="notice"><%= notice %></p>

<h1>Home Infos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Phone</th>
      <th>Cibil score</th>
      <th>Net monthly income</th>
      <th>Desired loan amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @home_infos.each do |home_info| %>
      <tr>
        <td><%= home_info.Name %></td>
        <td><%= home_info.Age %></td>
        <td><%= home_info.Phone %></td>
        <td><%= home_info.Cibil_Score %></td>
        <td><%= home_info.Net_Monthly_Income %></td>
        <td><%= home_info.Desired_Loan_Amount %></td>
        <td><%= link_to 'Show', home_info %></td>
        <td><%= link_to 'Edit', edit_home_info_path(home_info) %></td>
        <td><%= link_to 'Destroy', home_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Home Info', new_home_info_path %>
